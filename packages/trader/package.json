{
  "name": "@deriv-bot/trader",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./strategy": {
      "import": "./dist/strategy/index.js",
      "types": "./dist/strategy/index.d.ts"
    },
    "./strategies": {
      "import": "./dist/strategies/index.js",
      "types": "./dist/strategies/index.d.ts"
    },
    "./indicators": {
      "import": "./dist/indicators/index.js",
      "types": "./dist/indicators/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/main.ts",
    "start": "node dist/main.js",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "demo": "tsx src/scripts/run-rsi-bb-scalping-demo.ts",
    "demo:rsi-bb": "tsx src/scripts/run-rsi-bb-scalping-demo.ts",
    "demo:vdubus": "tsx src/scripts/run-vdubus-demo.ts",
    "demo:pivot": "tsx src/scripts/run-pivot-reversal-demo.ts",
    "demo:sr": "tsx src/scripts/run-support-resistance-demo.ts",
    "demo:squeeze": "tsx src/scripts/run-bb-squeeze.ts",
    "demo:squeeze-mr": "tsx src/scripts/run-bb-squeeze-mr.ts",
    "demo:keltner-mr": "tsx src/scripts/run-keltner-mr.ts",
    "demo:hybrid-mtf": "tsx src/scripts/run-hybrid-mtf.ts",
    "demo:fvg": "tsx src/scripts/run-fvg.ts",
    "data:fetch": "tsx src/scripts/fetch-historical-data.ts",
    "optimize:r75": "tsx src/scripts/optimize-r75.ts",
    "optimize:all": "tsx src/scripts/optimize-all-assets.ts",
    "backtest:squeeze:fetch": "tsx src/scripts/run-backtest-squeeze.ts",
    "backtest:run": "tsx src/scripts/run-unified-backtest.ts",
    "backtest:squeeze:deep": "tsx src/backtest/deep-loss-analysis.ts",
    "backtest:test-improvements": "tsx src/backtest/test-improvements.ts",
    "dashboard": "tsx src/scripts/run-dashboard-simple.ts",
    "dashboard:legacy": "tsx src/scripts/run-dashboard.ts",
    "test:validation": "tsx src/scripts/run-validation-test.ts",
    "analyze:forex": "tsx src/scripts/forex-market-characterization.ts",
    "analyze:regime": "tsx src/scripts/analyze-market-regimes.ts",
    "data:fetch:forex": "tsx src/scripts/fetch-forex-historical.ts",
    "data:fetch:calendar": "tsx src/scripts/fetch-economic-calendar.ts",
    "analyze:news": "tsx src/scripts/analyze-news-impact.ts",
    "compare:mr": "tsx src/scripts/compare-mr-strategies.ts",
    "compare:mr:full": "RUN_MC=true RUN_OOS=true tsx src/scripts/compare-mr-strategies.ts"
  },
  "dependencies": {
    "@deriv-bot/gateway": "workspace:*",
    "@deriv-bot/shared": "workspace:*",
    "@deriv/deriv-api": "^1.0.15",
    "dotenv": "^16.6.1",
    "ink": "^6.3.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "technicalindicators": "^3.1.0",
    "trading-signals": "^7.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@prisma/client": "^5.22.0",
    "@types/node": "^20.10.0",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "@vitest/ui": "^1.0.4",
    "prisma": "^5.22.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}
